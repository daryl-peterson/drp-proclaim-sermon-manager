{
	"name": "dpeterson/wp-sermon-manager",
	"type": "project",
	"autoload": {
		"psr-4": {
			"DRPSermonManager\\": "src/",
			"DRPSermonManager\\Abstracts\\": "src/Core/Abstracts",
			"DRPSermonManager\\Constants\\": "src/Core/Constants",
			"DRPSermonManager\\Exceptions\\": "src/Core/Exceptions",
			"DRPSermonManager\\Interfaces\\": "src/Core/Interfaces",
			"DRPSermonManager\\Logging\\": "src/Core/Logging",
			"DRPSermonManager\\Traits\\": "src/Core/Traits",
			"DRPSermonManager\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "daryl-peterson",
			"email": "daryl.peterson@gmail.com"
		}
	],
	"classmap-authoritative": true,
	"require": {
		"cmb2/cmb2": "^2.11",
		"psr/container": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.3",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
		"phpcompatibility/phpcompatibility-wp": ">=2.1.5",
		"wp-coding-standards/wpcs": "^3.1"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}